{
  "name": "ranking-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "shadcn-ui": "npx shadcn-ui@0.8.0",
